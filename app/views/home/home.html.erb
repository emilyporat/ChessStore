<% if logged_in? %>

	<% if current_user.role == "admin" %>
	  <%= render :partial => 'admin_dashboard' %>

	<% elsif current_user.role == "manager" %>
		<%= render :partial => 'manager_dashboard' %>

	<% elsif current_user.role == "shipper" %>
	  <%= render :partial => 'shipper_dashboard' %>

	<% elsif current_user.role == "customer" %>
	  <%= render :partial => 'customer_dashboard' %>

	<% end %>

<% else  %>
	<%= render :partial => 'general_home' %>
<% end %>